year <- sub("-.*", "", meta$Date) 
note <- sprintf("R package version %s", meta$Version) 
bibentry(bibtype = "Manual",          
         title = "{ZPDGrowthTrajectories}: Simulate achievement growth trajectories using a theoretical model of academic achievement based on the zone of proximal development (ZPD)",          
         author = c(person("Matthew", "McBee"))
         year = year,          
         note = note,          
         url = "https://github.com/mcbeem/ZPDGrowthTrajectories")